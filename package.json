{
  "name": "fal-ai-studio",
  "version": "1.0.0",
  "description": "Professional AI Image Generation Studio with Fal.ai Integration - Text-to-Image & Image-to-Image Editing",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "keywords": [
    "electron",
    "ai",
    "image-generation",
    "fal.ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "^26.6.10",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "@fal-ai/client": "^1.6.2",
    "electron-store": "^8.2.0"
  },
  "build": {
    "appId": "com.example.ai-image-generator",
    "productName": "AI Image Generator",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.graphics-design"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
